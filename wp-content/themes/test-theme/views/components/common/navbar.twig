{# Get menu items. #}
{% set navbarItems = ew_get_nav_menu_items('main-navigation-menu') %}

<div class="c-navbar">

    <div class="c-container">

        <div class="navbar-logo-container">

            <span>LOGO HERE</span>

        </div>

        {% if navbarItems|length > 0 %}

            <div class="navbar-items-container">

                {% for navbarItem in navbarItems %}

                    <a href="{{ navbarItem['link'] }}" class="navbar-item">

                        {{ navbarItem['label'] }}

                    </a>

                {% endfor %}

            </div>

        {% endif %}

    </div>

</div>